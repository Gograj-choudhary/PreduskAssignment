{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "MONGODB_URL": "@mongodb_url",
    "JWT_SECRET": "@jwt_secret",
    "REFRESH_TOKEN_SECRET": "@refresh_token_secret"
  }
}